<form class="form-horizontal">
    <div class="control-group">
        <label class="control-label">Url</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.homeassistantbuttons.url">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label">Token</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.homeassistantbuttons.token">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label">Lights</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.homeassistantbuttons.lightsid">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label">Printer</label>
        <div class="controls">
            <input type="text" class="input-block-level" data-bind="value: settings.plugins.homeassistantbuttons.printerid">
        </div>
    </div>
</form>
